# 이 파일을 application-secret.yml로 복사한 후 값을 입력하세요.
# cp application-secret.yml.example application-secret.yml

spring:
  datasource:
    username: your-db-username
    password: your-db-password
  security:
    oauth2:
      client:
        registration:
          google:
            client-id: your-google-client-id.apps.googleusercontent.com
            client-secret: your-google-client-secret

jwt:
  secret-key: your-jwt-secret-key-at-least-256-bits

aes:
  secret-key: your-aes-secret-key-16-bytes

google:
  places:
    api-key: your-google-places-api-key
